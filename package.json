{
  "name": "rualive-email-worker",
  "version": "1.0.0",
  "description": "RuAlive Email Notification Worker",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "dev:frontend": "vite --config vite.config.ts",
    "build:frontend": "vite build --config vite.config.ts",
    "predeploy": "node scripts/force-update.js",
    "deploy": "npm run predeploy && wrangler deploy",
    "deploy:landing": "wrangler deploy",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create rualive",
    "db:migrate": "wrangler d1 execute rualive --file=./schema.sql",
    "kv:create": "wrangler kv:namespace create KV",
    "kv:create-preview": "wrangler kv:namespace create KV --preview",
    "verify:build": "node scripts/verify-build.js",
    "deploy:full": "node scripts/deploy-full.js"
  }, "keywords": ["cloudflare", "worker", "email", "notification"], "author": "", "license": "ISC", "devDependencies": {"wrangler": "^3.114.17", "vite": "^5.0.0", "@vitejs/plugin-react": "^4.2.0", "typescript": "^5.3.0"}, "dependencies": {"bcryptjs": "^2.4.3", "react": "^19.0.0", "react-dom": "^19.0.0"}}
